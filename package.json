{
	"name": "pkg-name",
	"version": "0.0.0",
	"description": "",
	"keywords": [],
	"homepage": "https://github.com/sinanbekar/pkg-name#readme",
	"bugs": "https://github.com/sinanbekar/pkg-name/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sinanbekar/pkg-name.git"
	},
	"license": "MIT",
	"author": "Sinan Bekar <sinanbekar.work@gmail.com>",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"format": "pnpm run --parallel /^format:.*/",
		"format:eslint": "pnpm lint:eslint --fix",
		"format:prettier": "pnpm lint:prettier --write",
		"lint": "pnpm run --parallel /^lint:.*/",
		"lint:eslint": "eslint --cache .",
		"lint:prettier": "prettier --check .",
		"lint:types": "tsc --noEmit",
		"prepublishOnly": "pnpm run build",
		"release": "bumpp && pnpm publish",
		"test": "vitest"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@stylistic/eslint-plugin": "^5.2.3",
		"@types/node": "^20.19.4",
		"bumpp": "^10.2.2",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.6.2",
		"tsdown": "^0.13.3",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vitest": "^3.2.4"
	},
	"packageManager": "pnpm@10.14.0",
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": "20.19.4",
			"onFail": "download"
		}
	}
}
